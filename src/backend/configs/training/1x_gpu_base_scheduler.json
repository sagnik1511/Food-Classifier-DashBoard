{
    "job_name": "1x_gpu_scheduler",
    "device": "cuda",
    "loss": "cross_entropy",
    "optimizer": {
        "name": "adam",
        "params": {
            "lr": 0.0001,
            "weight_decay": 0.000001
        }
    },
    "metrics": ["accuracy", "precision", "recall", "f1_score"],
    "batch_size": 32,
    "max_epochs": 50,
    "callback": {
        "tracker": "f1_score",
        "on": "validation"
    }
}